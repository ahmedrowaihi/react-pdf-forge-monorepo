  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedrowaihi/react-pdf-forge-monorepo.git",
    "directory": "packages/templates"
  },
  "keywords": [
    "react",
    "pdf",
    "templates",
    "utilities"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "22.14.1",
    "tsdown": "0.15.12",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
