{
  "name": "@ahmedrowaihi/pdf-forge-templates",
  "version": "2.0.0-canary.4",
  "description": "Template discovery and metadata utilities for PDF Forge",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
